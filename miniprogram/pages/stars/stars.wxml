<!--miniprogram/pages/searchlist/searchlist.wxml-->
<view class='aa'>
   <text class='bb'>我的收藏</text>
</view>
<block wx:for="{{final}}" wx:key="key">
<van-panel  title="{{item.name}}" desc="类型: {{item.type}}" use-footer-slot>
  
  <view class="van-multi-ellipsis--l3" bindtap="navigateToInfo" data-id="{{item._id}}">
  <van-card
  title="{{item.date}}"
  thumb="{{ item.pic }}"
>
<view slot="desc" class="van-multi-ellipsis--l3" bindtap="navigateToInfo" data-id="{{item._id}}" style="font-size: 12px;">{{item.intro}}</view>
</van-card>
  </view>
  <view style="display: flex;justify-content:space-between">
  
  <view  class="sp" bindtap="navigateToInfo" data-id="{{item._id}}" style="color:#C0C0C0;">点击查看详情<van-icon name="arrow" color="#c0c0c0"/>
  </view>
  </view>
  <view slot="footer">

  </view>
</van-panel>
</block>
